nextflow_process {

    name "Test Process SENTIEON_BWAINDEX"
    script "../main.nf"
    process "SENTIEON_BWAINDEX"
    tag "modules"
    tag "modules_nfcore"
    tag "sentieon"
    tag "sentieon/bwaindex"

    test("test sentieon bwaindex") {

        when {
            process {
                """
                input[0] = [ [ id: 'test' ], file(params.modules_testdata_base_path + 'genomics/sarscov2/genome/genome.fasta', checkIfExists: true) ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
